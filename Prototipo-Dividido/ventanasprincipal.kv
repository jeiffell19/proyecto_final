#coding: utf8
<Principal>:
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'data/logouipapp.png'
    Label:
        text:
            ('[b]Principal[/b]')
        markup: True
        font_size: 50        
        size_hint:(None, None)
        size:(100, 44)
        pos_hint:{'center_x': .5 , 'center_y': .78} 
    Button:
        # Carrera
        text:'LICENCIATURAS'
        # Posicion
        size_hint:(None, None)
        size:(250, 50)
        pos_hint:{'center_x': .5,'center_y': .60}
        on_release: root.manager.current=('lincenciatura')
    Button:
        # Carrera
        text:'LICENCIATURAS  PARA EJECUTIVOS'
        # Posicion
        size_hint:(None, None)
        size:(250, 50)
        pos_hint:{'center_x': .5,'center_y': .50}
        on_release: root.manager.current=('lincenciaturaPro') 
    Button:
        # Carrera
        text:'MAESTRIA'
        # Posicion
        size_hint:(None, None)
        size:(250, 50)
        pos_hint:{'center_x': .5,'center_y': .40}
        on_release: root.manager.current=('maestria') 
    Button:
        # Carrera
        text:'POSTGRADOS'
        # Posicion
        size_hint:(None, None)
        size:(250, 50)
        pos_hint:{'center_x': .5,'center_y': .30}
        on_release: root.manager.current=('postgrado')
    Button:
        # Carrera
        text:'DIPLOMADOS Y CURSOS'
        # Posicion
        size_hint:(None, None)
        size:(250, 50)
        pos_hint:{'center_x': .5,'center_y': .20}    
        on_release: root.manager.current=('diplomado')