#DECLARACION DE FUNCIONES REUTILIZABLE 
#Propiedades para las titulos  
<Titulo@Label>
    markup: True
    # font_name: 'snap____.ttf' 
    font_size: 25        
    size_hint:(None, None)
    size:(100, 44)
    pos_hint:{'center_x': .5 , 'center_y': .90}

# Agragarmos cada Oferta academica que contendra las propiedades basicas
<Agregar_Oferta_Academica@Button>
    # font_name: 'britanic.ttf'
    pos_hint:{'center_x': .5,'center_y': .7}
    size_hint:(None, None)
    size:(250, 50)

#Reutilizan Butones y dando un nombre 
<Agregar_Falculta@Agregar_Oferta_Academica>
    text: 'aqui para una facultades'

#heredamos todos los atributos y solo cambiamos el text 
<Agregar_Carrera@Agregar_Oferta_Academica>
    text: 'aqui para una carrera'
#Declaracion  ventana de Facultades
<Falcultades@Screen>
  
#Declaracion de Carreras
<Carreras@Screen>

# Esquemas de los label que lleban los nombre ej Resolucion, Carrera
<TextoInfo@Label> 
    text: 'aqui va ir un label'
    font_size: 25             
    size_hint:(.35, None)
    size:(100, 40)
    pos_hint:{'right_top': .10,'center_y': .75}

#Esquema des los Textinput que sera donde se va a mostarar la carrera
<ResultadoInfo@TextInput>
    text: 'aqui va ir un texto input: '
    size_hint:(.65, None)
    size:(100, 40)
    pos_hint:{'center_x': .60,'center_y': .75}
    disabled: True  

<Boton_Atras@Button>
    text: 'Atras'
    background_color: (0, 0, 255,.5)
    size_hint:(None, None)
    size:(150, 30)
    pos_hint:{'center_x': .25,'center_y': .03}
<Boton_Siguiente@Button>
    text: 'Menu Principal'
    background_color: (0, 0, 255,.5)
    size_hint:(None, None)
    size:(150, 30)
    pos_hint:{'center_x': .75,'center_y': .03}

# No servira para mandarle la imagen de plande estudios de la carrea
<Plan@Image>
    size: self.size
    source: 'data/1.png'
    size: self.texture_size
# Agregamos la imagen de Plan y la ingresamos en Widgit que se pueda mover
<PlanDeEstudio@Screen>
    Widget: 
        Scatter:
            size_hint: None, None
            size: image.size
